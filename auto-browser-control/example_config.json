{
  "chrome": {
    "remote_debugging_port": 9222,
    "startup_timeout": 10
  },
  "automation": {
    "default_timeout": 30,
    "retry_count": 3,
    "retry_delay": 2.0
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file": "web_automation.log"
  }
}